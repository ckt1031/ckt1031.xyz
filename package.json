{
    "private": true,
    "name": "cktidy",
    "version": "0.0.27",
    "description": "Cktsun's Personal Website",
    "license": "MIT",
    "author": "cktsun1031",
    "type": "module",
    "homepage": "https://github.com/cktsun1031/cktidy#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cktsun1031/cktidy.git"
    },
    "bugs": {
        "url": "https://github.com/cktsun1031/cktidy/issues"
    },
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "build": "next build && next-sitemap",
        "commit": "cz",
        "dev": "next dev -p 4072",
        "format": "prettier . -w",
        "lint": "tsc && next lint --fix",
        "lint:type": "tsc",
        "prepare": "husky install",
        "start": "next start",
        "e2e": "playwright test"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "packageManager": "pnpm@7.16.0",
    "dependencies": {
        "@next/env": "13.0.3",
        "@next/font": "13.0.3",
        "@sanity/image-url": "1.0.1",
        "@sanity/webhook": "2.0.0",
        "@sentry/nextjs": "7.19.0",
        "clsx": "1.2.1",
        "cssnano": "5.1.14",
        "csstype": "3.1.1",
        "dayjs": "1.11.6",
        "feed": "4.2.2",
        "framer-motion": "7.6.6",
        "next": "13.0.3",
        "next-mdx-remote": "4.2.0",
        "next-sanity": "1.0.9",
        "next-seo": "5.14.1",
        "next-sitemap": "3.1.31",
        "next-themes": "0.2.1",
        "nextjs-progressbar": "0.0.16",
        "nextjs-routes": "1.0.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-ga": "3.3.1",
        "react-icons": "4.6.0",
        "react-photo-view": "1.2.2",
        "react-use": "17.4.0",
        "reading-time": "1.5.0",
        "rehype-autolink-headings": "6.1.1",
        "rehype-code-titles": "1.1.0",
        "rehype-external-img-size": "0.1.0",
        "rehype-external-links": "2.0.1",
        "rehype-slug": "5.1.0",
        "remark-gfm": "3.0.1",
        "remark-normalize-headings": "3.0.1",
        "remark-parse": "10.0.1",
        "remark-unwrap-images": "3.0.1"
    },
    "devDependencies": {
        "@playwright/test": "1.27.1",
        "@tailwindcss/typography": "0.5.8",
        "@types/node": "18.11.9",
        "@types/react": "18.0.25",
        "@types/react-dom": "18.0.8",
        "@typescript-eslint/eslint-plugin": "5.42.1",
        "@typescript-eslint/parser": "5.42.1",
        "autoprefixer": "10.4.13",
        "commitizen": "4.2.5",
        "cross-env": "7.0.3",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "8.27.0",
        "eslint-config-next": "13.0.3",
        "eslint-plugin-jsx-a11y": "6.6.1",
        "eslint-plugin-react": "7.31.10",
        "eslint-plugin-simple-import-sort": "8.0.0",
        "eslint-plugin-tailwindcss": "3.6.2",
        "husky": "8.0.2",
        "lint-staged": "13.0.3",
        "postcss": "8.4.19",
        "prettier": "2.7.1",
        "prettier-plugin-tailwindcss": "0.1.13",
        "sanity-codegen": "0.9.8",
        "tailwindcss": "3.2.4",
        "typescript": "4.8.4"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint"
        ]
    }
