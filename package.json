{
    "private": true,
    "name": "cktidy.xyz",
    "version": "2023.01.02",
    "description": "Cktsun's Personal Website",
    "license": "MIT",
    "author": "cktsun1031",
    "type": "module",
    "homepage": "https://github.com/cktsun1031/cktidy#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cktsun1031/cktidy.git"
    },
    "bugs": {
        "url": "https://github.com/cktsun1031/cktidy/issues"
    },
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "build": "next build",
        "commit": "cz",
        "dev": "next dev -p 4072",
        "format": "prettier . -w",
        "lint": "tsc && eslint --fix .",
        "prepare": "husky install",
        "start": "next start"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "engines": {
        "node": ">=18.0.0",
        "pnpm": ">=7.20.0"
    },
    "packageManager": "pnpm@7.22.0",
    "dependencies": {
        "@headlessui/react": "1.7.7",
        "@next/env": "13.1.1",
        "@next/font": "13.1.1",
        "@sanity/image-url": "1.0.1",
        "@sanity/webhook": "2.0.0",
        "@sentry/nextjs": "7.29.0",
        "clsx": "1.2.1",
        "cssnano": "5.1.14",
        "csstype": "3.1.1",
        "dayjs": "1.11.7",
        "feed": "4.2.2",
        "fuse.js": "6.6.2",
        "next": "13.1.1",
        "next-mdx-remote": "4.2.1",
        "next-sanity": "3.1.9",
        "next-seo": "5.15.0",
        "next-sitemap": "3.1.44",
        "next-themes": "0.2.1",
        "nextjs-progressbar": "0.0.16",
        "nextjs-routes": "1.0.4",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-icons": "4.7.1",
        "react-photo-view": "1.2.3",
        "react-snowfall": "1.2.1",
        "react-use": "17.4.0",
        "reading-time": "1.5.0",
        "rehype-autolink-headings": "6.1.1",
        "rehype-code-titles": "1.1.0",
        "rehype-external-img-size": "0.1.0",
        "rehype-external-links": "2.0.1",
        "rehype-slug": "5.1.0",
        "remark-gfm": "3.0.1",
        "remark-normalize-headings": "3.0.1",
        "remark-parse": "10.0.1",
        "remark-unwrap-images": "3.0.1",
        "s-ago": "2.2.0"
    },
    "devDependencies": {
        "@commitlint/cli": "17.4.0",
        "@commitlint/config-conventional": "17.4.0",
        "@tailwindcss/typography": "0.5.8",
        "@types/node": "18.11.18",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.10",
        "@typescript-eslint/eslint-plugin": "5.48.0",
        "@typescript-eslint/parser": "5.48.0",
        "autoprefixer": "10.4.13",
        "commitizen": "4.2.6",
        "cross-env": "7.0.3",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "8.31.0",
        "eslint-config-next": "13.1.1",
        "eslint-plugin-deprecation": "1.3.3",
        "eslint-plugin-jsx-a11y": "6.6.1",
        "eslint-plugin-react": "7.31.11",
        "eslint-plugin-simple-import-sort": "8.0.0",
        "eslint-plugin-tailwindcss": "3.8.0",
        "husky": "8.0.3",
        "lint-staged": "13.1.0",
        "postcss": "8.4.21",
        "prettier": "2.8.2",
        "prettier-plugin-tailwindcss": "0.2.1",
        "sanity-codegen": "0.9.8",
        "tailwindcss": "3.2.4",
        "typescript": "4.9.4"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint"
        ],
        "*.{yml,cjs,mjs,js,jsx,ts,tsx,json,css,html,md}": [
            "prettier -c"
        ]
    }
}
